@using ljgb.Common.Responses;
@{

    var data = (ProvinsiResponse)ViewData["DetailResponse"];
}


<div class="form-group">
    <input type="hidden" id="ModelID" name="id" value="@data.Model.ID">
    <label class="form-label" for="simpleinput">Nama Provinsi</label>

    <input type="text" id="Nama" name="Nama" value="@data.Model.Nama" class="form-control">
</div>
<div class="form-group">
    <label class="form-label" for="example-textarea" value="@data.Model.Description" >Deskripsi</label>
    <textarea class="form-control" id="DeskripsiProvinsi" name="Description"  rows="5">@data.Model.Description</textarea>
</div>