@section Scripts {
    <script type="text/javascript">

        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if (charCode > 31 && (charCode < 48 || charCode > 57))
                return false;

            return true;
        }
        function formatRupiah(input) {


            var number_string = input.toString(),
                sisa = number_string.length % 3,
                rupiah = number_string.substr(0, sisa),
                ribuan = number_string.substr(sisa).match(/\d{3}/g);
            if (ribuan) {
                separator = sisa ? '.' : '';
                rupiah += separator + ribuan.join('.');
            }
            return rupiah;
        }
        $(document).ready(function () {
            moment.locale('ID');
            var url = "@ViewBag.url_api";

            $('#tableTransaksi').dataTable({
                language: {
                    processing: "<div class='overlay custom-loader-background'><i class='fa fa-cog fa-spin custom-loader-color'></i></div>"
                },
                processing: true,
                serverSide: true,
                initComplete: function () {
                    $(this.api().table().container()).find("input").parent().wrap("<form>").parent().attr("autocomplete", "off");
                },
                ajax: {
                    //url: url + "User/GetSalesman",
                    url: url + "Transaction/GetAll",
                    contentType: 'application/json; charset=utf-8',
                    type: "POST",
                    dataType: "json",
                    beforeSend: function (xhr) {
                        xhr.setRequestHeader('Authorization', 'Bearer ' + Cookies.get("access_token"));
                    },
                    data: function (d) {
                        // Grab form values containing user options

                        return JSON.stringify(d);
                    },
                    dataSrc: function (json) {
                        json.draw = json.draw;
                        json.recordsTotal = json.recordsTotal;
                        json.recordsFiltered = json.recordsFiltered;
                        json.data = json.listTransaction;
                        var return_data = json;
                        return return_data.data;
                    }
                },
                responsive: true,
                columns: [
                    {
                        data: "namaStatus",
                        mRender: function (a, e, t) {
                            return '<a data-TransactionID="' + t.id + '" data-toggle="modal" data-target="#ModalTrackingStatus" >' + t.namaStatus + ' </a> '

                        },
                        width: "10%"
                    },

                    {

                        mRender: function (a, e, t) {
                            return '<a data-TransactionID="' + t.id + '"  data-nama="' + t.namaPembeli + '" data-instagram="' + t.igPembeli + '" data-facebook="' + t.facebookPembeli + '" data-alamat="' + t.alamatPembeli + '" data-email="' + t.emailPembeli + '" data-telp="' + t.telpPembeli + '" data-toggle="modal" data-target="#ModalEdit" >' + t.namaPembeli + ' </a> '

                        },
                        width: "15%"
                    },
                    {
                        mRender: function (a, e, t) {
                            return '<a data-TransactionID="' + t.id + '"  data-nama="' + t.namaPenjual + '" data-instagram="' + t.igPenjual + '" data-facebook="' + t.facebookPenjual + '" data-alamat="' + t.alamatPenjual + '" data-email="' + t.emailPenjual + '" data-telp="' + t.telpPenjual + '" data-toggle="modal" data-target="#ModalEdit" >' + t.namaPenjual + ' </a> '

                        },
                        width: "15%"
                    },
                    {
                        data: "namaBarang",
                        width: "15%"
                    },
                    {
                        mRender: function (a, e, t) {
                            return '<a data-TransactionID="' + t.id + '"  data-nama="' + t.namaDealer + '"  data-kode="' + t.kodeDealer + '" data-alamat="' + t.alamatDealer + '"  data-kota="' + t.kotaDealer + '"  data-Telpon="' + t.telpDealer + '"  data-toggle="modal" data-target="#ModalDealer" >' + t.namaPenjual + ' </a> '

                        },
                        width: "10%"
                    },
                    {
                        data: "tipePenawaran",
                        width: "10%"
                    },
                    {
                        mRender: function (a, e, t) {
                            return 'Rp.' +formatRupiah(t.hargaNego);
                        },
                        width: "10%"
                    },
                    {
                        mRender: function (a, e, t) {
                            return 'Rp.'+ formatRupiah(t.hargaOTR);
                        },
                        width: "10%"
                    },
                    {
                        mRender: function (a, e, t) {
                            if (t.namaStatus == "Canceled"|| t.namaStatus == "Done") {
                                return '';
                            }
                            else {
                                if (t.namaStatus == 'Waiting Verification') {
                                    return '<a class="btn btn-xs btn-success waves-effect waves-themed" data-TransactionID="' + t.id + '"  data-nama="' + t.namaPembeli + '" data-instagram="' + t.igPembeli + '" data-facebook="' + t.facebookPembeli + '" data-alamat="' + t.alamatPembeli + '" data-email="' + t.emailPembeli + '" data-telp="' + t.telpPembeli + '" data-toggle="modal" data-target="#ModalWaitingVerification" ><i class="fas fa-edit">' + t.namaLangkah + ' </i></a> '
                                        + '<a class="btn btn-xs btn-danger waves-effect waves-themed"  data-TransactionID="' + t.id + '" data-toggle="modal" data-target="#ModalCancel"><i class="fas fa-trash-alt">Cancel </i></a>';
                                }
                             return '<a class="btn btn-xs btn-success waves-effect waves-themed"  data-TransactionID="' + t.id + '" data-toggle="modal" data-target="#ModalSave" ><i class="fas fa-edit">' + t.namaLangkah + ' </i></a> '
                                 + '<a class="btn btn-xs btn-danger waves-effect waves-themed"  data-TransactionID="' + t.id + '" data-toggle="modal" data-target="#ModalCancel"><i class="fas fa-trash-alt">Cancel </i></a>';
                             }
                        },
                        width: "5%"
                    }
                ]
            });

            function resetModal() {
                var $t = $('[data-dismiss=modal]'),
                    target = $t[0].href || $t.data("target") || $t.parents('.modal') || [];
                $(target)
                    .find("input,textarea,select")
                    .val('')
                    .end()
                    .find("input[type=checkbox], input[type=radio]")
                    .prop("checked", "")
                    .end();
            }

            $("#ModalSave").on("show.bs.modal", function (e) {
                $('#TransactionID').val(e.relatedTarget.getAttribute('data-TransactionID'));
            });
            $("#ModalCancel").on("show.bs.modal", function (e) {
                $('#TransactionID').val(e.relatedTarget.getAttribute('data-TransactionID'));
            });

            $("#ModalTrackingStatus").on("show.bs.modal", function (e) {
                resetModal();
                $('#TransactionID').val(e.relatedTarget.getAttribute('data-TransactionID'));

                $('#tableJurnalTransaksi').dataTable({
                    language: {
                        processing: "<div class='overlay custom-loader-background'><i class='fa fa-cog fa-spin custom-loader-color'></i></div>"
                    },
                    processing: false,
                    serverSide: false,
                    searching: false,
                    paging: false,
                    ordering: false,
                    initComplete: function () {
                        $(this.api().table().container()).find("input").parent().wrap("<form>").parent().attr("autocomplete", "off");
                    },
                    ajax: {
                        //url: url + "User/GetSalesman",
                        url: url + "Transaction/GetHistory",
                        contentType: 'application/json; charset=utf-8',
                        type: "POST",
                        dataType: "json",
                        beforeSend: function (xhr) {
                            xhr.setRequestHeader('Authorization', 'Bearer ' + Cookies.get("access_token"));
                        },
                        data: function (d) {
                            // Grab form values containing user options
                            var values = {
                                id: $("#TransactionID").val()
                            };
                            return JSON.stringify(values);
                        },
                        dataSrc: function (json) {
                            json.draw = json.draw;
                            json.recordsTotal = json.recordsTotal;
                            json.recordsFiltered = json.recordsFiltered;
                            json.data = json.listHistory;
                            var return_data = json;
                            return return_data.data;
                        }
                    },
                    responsive: true,
                    columns: [
                        {
                            data: "transactionStatus",
                            width: "10%"
                        },
                        {
                            data: "transactionStep",
                            width: "10%"
                        },
                        {
                            mRender: function(a,e,t) {
                                date = new Date(t.created);
                                var shortDate = date.getDate() + '-' + (date.getMonth() + 1) + '-' + date.getFullYear();
                                return shortDate;
                            },
                            data: "created",
                            width: "10%"
                        },
                    ]
                });
            });

            $("#ModalTrackingStatus").on("hidden.bs.modal", function (e) {
                $('#tableJurnalTransaksi').DataTable().clear().destroy();
            });
            $('#ModalEdit').on('show.bs.modal', function (e) {
                resetModal();
               
