            var forms = document.getElementsByClassName('needs-validation');
            var buttonWaitingVerification = document.getElementById("btn-saveWaitingVerification");

            var validation = Array.prototype.filter.call(forms, function (form) {
                buttonWaitingVerification.addEventListener('click', function (event) {
                    if (form.checkValidity() === false) {
                        event.preventDefault();
                        event.stopPropagation();
                    } else {
                        $("#ModalWaitingVerification").modal('hide');
                        $(document.body).removeClass('modal-open');
                        $('.modal-backdrop').remove();
                        LoadingMask.show();
                        var values = {
                            ID: $('#TransactionID').val(),
                            Email: $("#modalVerificationEmail").val(),
                            Name: $("#modalVerificationName").val(),
                            Telp: $("#modalVerificationTelp").val(),
                            Alamat: $("#modalVerificationAlamat").val(),
                            Facebook: $("#modalVerificationFacebook").val(),
                            IG: $("#modalVerificationFacebook").val(),
                          
                        };

                        $.ajax({
                            url: url + "Transaction/ConfirmWaitingVerification",
                            contentType: 'application/json',
                            dataType: 'json',
                            type: "POST",
                            beforeSend: function (xhr) {
                                xhr.setRequestHeader('Authorization', 'Bearer ' + Cookies.get("access_token"));
                            },
                            data: JSON.stringify(values),
                            success: function (response) {
                                if (response.isSuccess) {
                                    LoadingMask.hide();
                                    toastr["success"](response.message);
                                    var table = $('#tableTransaksi').DataTable();
                                    table.draw(false);
                                } else {
                                    LoadingMask.hide();
                                    toastr["error"](response.message);
                                }
                            },
                            error: function (jqXHR, text, errorThrown) {
                                console.log(jqXHR + " " + text + " " + errorThrown);
                            }
                        });
                    }
                    form.classList.add('was-validated');
                });
            });

            $('#save').on('click', function (e) {

                if ($("#VerifiedDate").val() !== "" && $("#VerifiedBy").val() !== "") {
                    var userData = {
                        DetailID: ($("#UserDetailID").val()) ? $("#UserDetailID").val() : 0,
                        VerifiedDate: $("#VerifiedDate").val(),
                        VerifiedBy: $("#VerifiedBy").val()
                    }

                    $.ajax({
                        url: url + "User/SaveUserDetail",
                        type: "POST",
                        dataType: 'json',
                        beforeSend: function (xhr) {
                            xhr.setRequestHeader('Authorization', 'Bearer ' + Cookies.get("access_token"));
                        },
                        contentType: "application/json",
                        data: JSON.stringify(userData),
                        success: function (response) {
                            if (response.isSuccess) {
                                $("#ModalEdit").modal('hide');
                                $(document.body).removeClass('modal-open');
                                $('.modal-backdrop').remove();
                                toastr["success"](response.message);
                                $('#tbuser').DataTable().ajax.reload();
                            } else {
                                toastr["error"](response.message);
                            }
                        }
                    });
                } else {
                    toastr["error"]("Please fill Verified Date and Verified By!.");
                }
            });

