                $('#NegoBarangID').val(e.relatedTarget.getAttribute('data-id'));
                $('#dropdownMerkEdit').val(e.relatedTarget.getAttribute('data-merkID'));
                $('#BarangID').val(e.relatedTarget.getAttribute('data-barangID'));


                var valuesMerkID = {

