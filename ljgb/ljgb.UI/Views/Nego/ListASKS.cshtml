@section Scripts {
    <script type="text/javascript">
        function isNumberKey(evt) {
            var charCode = (evt.which) ? evt.which : event.keyCode
            if (charCode > 31 && (charCode < 48 || charCode > 57))
                return false;

            return true;
        }
        function formatRupiahWithSelector(input) {

            var number_without_separator = $(input).val().toString().split('.'),
                number_string = number_without_separator.join('').toString(),
                sisa = number_string.length % 3,
                rupiah = number_string.substr(0, sisa),
                ribuan = number_string.substr(sisa).match(/\d{3}/g);
            if (ribuan) {
                separator = sisa ? '.' : '';
                rupiah += separator + ribuan.join('.');
            }
            $(input).val(rupiah);
        }
        function formatRupiah(input) {


            var number_string = input.toString(),
                sisa = number_string.length % 3,
                rupiah = number_string.substr(0, sisa),
                ribuan = number_string.substr(sisa).match(/\d{3}/g);
            if (ribuan) {
                separator = sisa ? '.' : '';
                rupiah += separator + ribuan.join('.');
            }
            return rupiah;
        }
        $(document).ready(function () {
            moment.locale('ID');
            var url = "@ViewBag.url_api";
          
            $.ajax({
                url: url + "Merk/GetAllWithoutFilter",
                contentType: 'application/json',
                dataType: 'json',
                type: "GET",
                beforeSend: function (xhr) {
                    xhr.setRequestHeader('Authorization', 'Bearer ' + Cookies.get("access_token"));
                },
                success: function (response) {
                    if (response.isSuccess) {
                        $("#dropdownMerk").append($('<option></option>').attr('value', -1).text("Pilih Model"));
                        $("#dropdownMerkEdit").append($('<option></option>').attr('value', -1).text("Pilih Model"));
                        $.each(response.listModel, function (key, entry) {
                            $("#dropdownMerk").append($('<option></option>').attr('value', entry.id).text(entry.name));
                            $("#dropdownMerkEdit").append($('<option></option>').attr('value', entry.id).text(entry.name));
                        })
                    } else {
                        toastr["error"](response.message);
                    }
                },
                error: function (jqXHR, text, errorThrown) {
                    console.log(jqXHR + " " + text + " " + errorThrown);
                }
            });
           
            $('#tableAsk').dataTable({
                language: {
                    processing: "<div class='overlay custom-loader-background'><i class='fa fa-cog fa-spin custom-loader-color'></i></div>"
                },
                processing: true,
                serverSide: true,
                initComplete: function () {
                    $(this.api().table().container()).find("input").parent().wrap("<form>").parent().attr("autocomplete", "off");
                },
                beforeSend: function (xhr) {
                    xhr.setRequestHeader('Authorization', 'Bearer ' + Cookies.get("access_token"));
                },
                ajax: {
                    url: url + "NegoBarang/GetAllASK",
                    contentType: "application/json",
                    type: "POST",
                    dataType: "JSON",
                    beforeSend: function (xhr) {
                        xhr.setRequestHeader('Authorization', 'Bearer ' + Cookies.get("access_token"));
                    },
                    data: function (d) {
                        return JSON.stringify(d);
                    },
                    dataSrc: function (json) {
                        json.draw = json.draw;
                        json.recordsTotal = json.recordsTotal;
                        json.recordsFiltered = json.recordsFiltered;
                        json.data = json.listModel;
                        var return_data = json;
                        return return_data.data;
                    }
                },
                responsive: true,
                columns: [

                    {
                        data: "namaBarang",
                        width: "25%"
                    },
                    {
                        mRender: function (a, e, t) {
                            return 'Rp.' + formatRupiah(t.harga);
                        },
                        width: "15%"
                    },
                    {
                        mRender: function (a, e, t) {
                            return 'Rp.' + formatRupiah(t.hargaOTR);
                        },
                        width: "15%"
                    },
                    {
                        mRender: function (a, e, t) {
                            return '<a class="btn btn-xs btn-success waves-effect waves-themed"  data-id="' + t.id + '" data-barangID="'+t.barangID+'" data-merkID="' + t.merkID + '" data-modelID="' + t.modelBarangID + '"  data-typeID="' + t.typeBarangID + '"  data-harga="' + t.harga + '" data-hargaOTR="' + t.hargaOTR + '" data-warnaID="' + t.warnaID + '" data-toggle="modal" data-target="#ModalEdit"><i class="fas fa-edit"></i></a> '
